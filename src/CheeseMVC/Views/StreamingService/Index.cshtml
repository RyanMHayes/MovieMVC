@model List<MovieMVC.Models.MovieStreamingService>


<h2>Streaming Services</h2>

@foreach (var service in Model)
{
    <li>
        <ul>@service.StreamingService</ul>
    </li>
}


@if (Model.Count == 0)
{
    <p>No streaming services have been added yet!</p>
}


<a asp-controller="Genre" asp-action="Add">Add Genre</a> | <a asp-controller="Movie" asp-action="Add">Add Movie</a>
