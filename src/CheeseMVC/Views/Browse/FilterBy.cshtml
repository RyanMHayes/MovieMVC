@model MovieMVC.ViewModels.SearchMovieViewModel

<h1>@Model.Title</h1>

<table>
    @foreach (var field in Model.Fields)
    {
        <tr>
            <td>
                <a asp-controller="Browse"
                   asp-action="MovieListings"
                   asp-route-filter="@Model.Filter"
                   asp-route-value="@field.Value">@field.Value</a>
            </td>
        </tr>
    }
</table>
