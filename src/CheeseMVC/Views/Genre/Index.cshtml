@model List<MovieMVC.Models.MovieGenre>


<h2>Genres</h2>

@foreach (var genre in Model)
{
    <li>
        <ul>@genre.Genre</ul>
    </li>
}


@if (Model.Count == 0)
{
    <p>No genres have been added yet!</p>
}


<a asp-controller="Genre" asp-action="Add">Add Genre</a> | <a asp-controller="Movie" asp-action="Add">Add Movie</a>
